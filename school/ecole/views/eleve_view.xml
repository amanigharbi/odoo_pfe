<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <!-- Form View 1 Of eleve's Profiles -->
    <record id="view_eleve_eleve_form_1" model="ir.ui.view">
        <field name="nom">eleve.eleve.form.1</field>
        <field name="model">eleve.eleve</field>
        <field name="arch" type="xml">
            <form string="Informations Elèves" >
                <header>
                    <button class="oe_highlight oe_stat_button" name="%(ecole.action_mettre_fin_wizard)d" string="Mettre fin" type="action" icon="fa-ban" etats="terminé" groups="ecole.group_ecole_administration" />
                    <button class="oe_stat_button" name="set_ancien" string="Anciens" type="object" icon="fa-archive" etats="terminé" groups="ecole.group_ecole_administration" />
                    <button class="oe_stat_button" icon="fa-check" name="admission_terminé" type="object" string="Terminé" etats="Brouillon" groups="ecole.group_ecole_administration" />
                    <field name="etat" widget="statusbar" statusbar_visible="ancien,mettre_fin" attrs="{'invisible':[('etat','=','terminé')]}"/>
                </header>
                <sheet>
                    <div class="oe_button_box" name="buttons"></div>
                    <div class="oe_inline">
                        <newline />
                        <separator string="Informations Personnelles" />
                        <b>
                            <field name="reg_code" readonly="1" />
                        </b>
                        <br />
                        <field name="photo" widget="image" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}" class="oe_avatar oe_left" style="max-width:100px;max-height:117px;"/>
                        <div class="oe_title">
                            <group>
                                <field name="nom" placeholder="Prénom" nolabel="1" required="1" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <br />
                                <field name="nom_famille_eleve" placeholder="Nom Famille" nolabel="1" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <br />
                                <field name="pid" invisible="1"/>
                            </group>
                        </div>
                    </div>
                    <group>
                        <group string="Information Elèves">
                            <field name="eleve_code" readonly="1" />
                            <field name="annee" placeholder="Année" widget="selection" help="annee academique d'eleve" attrs="{'readonly':['|',('etat','in',['ancien','mettre_fin']),('enseignant_grp','=',True)]}"/>
                            <field name="ecole_id" placeholder="Ecole" widget="selection" help="Ecole d'eleve" attrs="{'readonly':['|',('etat','in',['ancien','mettre_fin']),('enseignant_grp','=',True)]}"/>
                        </group>
                        <group string="Informations Classe">
                            <field name="classe_id" placeholder="Classe" readonly="1" domain="[('ecole_id','=',ecole_id)]" widget="selection" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                            <field name="division_id" placeholder="Division" invisible="1" widget="selection" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                  </group>
                    </group>
                    <group>
                        <group>
                            <label for="street" string="Adresse" />
                            <div class="o_address_format">
                                <field name="street" class="o_address_street" placeholder="Rue..." attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="ville" placeholder="Ville" class="o_address_city" required="1" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="etat_id" domain="[('pays_id','=',pays_id)]" class="o_address_etat" required="1" placeholder="Etat" options="{&quot;no_open&quot;: True}" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="pays_id" placeholder="Pays" class="o_address_country,oe_no_button" options="{&quot;no_open&quot;: True}" required="1" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                            </div>
                        </group>
                        <group>
                            <field name="telephone" string="Numéro Télételephone" placeholder="Numéro Télételephone" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                            <field name="mobile" string="Numéro de Portable" placeholder="Numéro de Portable" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                            <field name="email"  placeholder="Adresse Email" String="Email" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                            <field name="website" string="Site Web" widget="url" placeholder="ex www.exemple.com" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                            <field name="enseignant_grp" invisible="1"/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Géneral" name="general">
                            <group>
                                <group>
                                    <field name="caste_id" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}" widget="selection" placeholder="Réligion/Caste"/>
                                    <field name="date_de_naissance" widget="date" placeholder="Date De Naissance" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                    <field name="sexe" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}" placeholder="Sexe" required="1"/>
                                    <field name="age"/>
                                </group>
                                <group>
                                    <field name="langue_maternelle" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"  widget="selection" placeholder="Lanque Maternelle"/>
                                    <field name="date_admission" widget="date" placeholder="Date d'admission" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}" readonly="1"/>
                                    <field name="etat_civil" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}" placeholder="État civil"/>
                                </group>
                                <newline />
                                <separator string="Détails des contacts d'urgence" colspan="4" />
                                <newline />
                                <group colspan="4" col="4">
                                    <field name="contact_telephone" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}" placeholder="Numéro de Télételephone"/>
                                    <field name="contact_portable" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}" placeholder="Numéro de Portable"/>
                                </group>
                                <newline />
                            </group>
                            <newline/>
                            <separator string="Motif de résiliation" attrs="{'invisible':[('etat','in',['Brouillon','terminé','ancien'])]}"/>
                            <group>
                                    <field name="mettre_fin_raison" attrs="{'invisible':[('etat','in',['Brouillon','terminé','ancien'])]}" readonly="1"/>
                              </group>
                        </page>
                        <page string="Parents">
                            <field name="parent_id"
                                        attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"
                                        context="{'form_view_ref':'ecole.view_parent_form','default_parent_ecole':1}"
                                        options="{&quot;no_open&quot;: True, &quot;no_create&quot;: True}"/>
                        </page>
                        <page string="Precedent Ecole">
                            <field name="precedent_ecole_ids" nolabel="1" colspan="4" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}">
                                <tree string="Details Precedent Ecole">
                                    <field name="nom" />
                                    <field name="numero_enseignant" />
                                    <field name="date_admission" />
                                    <field name="date_sortie" />
                                    <field name="course_id" widget="selection" />
                                </tree>
                                <form string="Details Precedent Ecole">
                                    <separator string="Details Precedent Ecole" />
                                    <group col="4" colspan="4">
                                        <field name="nom" placeholder="Nom Ecole" />
                                        <field name="numero_enseignant" placeholder="N° d'enregistrement" />
                                        <field name="date_admission" />
                                        <field name="date_sortie" />
                                        <field name="course_id" widget="selection"/>
                                        <newline />
                                        <separator string="Details Matières"/>
                                        <field name="ajouter_mat" nolabel="1" colspan="4" />
                                    </group>
                                </form>
                            </field>
                        </page>

                        <page string="Médicale">
                            <group>
                                <group string="Informations Sur Le Médecin">
                                    <field name="designation" placeholder="Désignation du médecin" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                    <field name="medecin_telephone" placeholder="Contact avec le médecin Non" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                </group>
                                <group string="Informations physiques">
                                    <field name="groupe_sanguin" placeholder="ex.B+,Ab+" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                    <field name="hauteur" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}" placeholder="hauteur"/>
                                    <field name="largeur" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}" placeholder="largeur"/>
                                </group>
                            </group>
                            <separator string="Le demandeur a-t-il des handicaps?" colspan="4" />
                            <group col="6" colspan="4">
                                <field name="yeux" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="oreille" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="nez_gorge" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="respiratoire" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="cardiovasculaire" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="neurologique" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="dermatologique" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                                <field name="pression_arterielle" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                            </group>
                            <separator string="Notes" colspan="4" />
                            <group col="6" colspan="4">
                                <field name="commentaire" placeholder="Notes sur le médical.." nolabel="1" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}"/>
                            </group>
                        </page>
                        <page string="Certifications">
                            <field name="certificat_ids" nolabel="1" colspan="4" attrs="{'readonly':[('etat','in',['ancien','mettre_fin'])]}">
                                <form string="Certification">
                                    <group col="4">
                                        <field name="description" placeholder="Description du certificat" />
                                        <field name="certi" />
                                    </group>
                                </form>
                                <tree string="Certifaicat" editable="top">
                                    <field name="description" required="1" placeholder="Entrer Déscription"/>
                                    <field name="certi" required="1" placeholder="Télécharger Cértificat"/>
                                </tree>
                            </field>
                        </page>
                         <page string="Desciplines" >
                            <field name="descplines_ids" nolabel="1" colspan="4"
                          >
                                <tree string="Despline">
                                    <field name="eleve_id" invisible="1" />
                                    <field name="matiere_id" required="1"/>
                                    <field name="device_datetime" required="1"/>
                                    <field name="status"/>

                                </tree>
                            </field>
                         </page>

              <page string="Sanctions Scolaires"  name="affiche">
                            <field name="sanctions_ids" nolabel="1" colspan="4">

                                <tree string="Sanction">
                                      <field name="eleve_id" invisible="1" />
                                    <field name="sanction" required="1" />
                                    <!--    <field name="sanction" required="1" widget="Selection"/>-->
                                    <field name="nombre" required="1"/>
                                </tree>
                            </field>
                         </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
    <!-- Tree View 1 Of eleve's Profiles -->
    <record id="view_eleve_eleve_tree_1" model="ir.ui.view">
        <field name="nom">eleve.eleve.tree.1</field>
        <field name="model">eleve.eleve</field>
        <field name="arch" type="xml">
            <tree string="Informations Elève" >
                <field name="pid" />
                <field name="nom" string="Nom Elève" />
                <field name="eleve_nom" invisible="1" />
                <field name="annee" widget="selection" />
                <field name="date_admission" />
                <field name="sexe" />
                <field name="etat" invisible="1"/>
                <field name="ecole_id" />
            </tree>
        </field>
    </record>
    <!-- Search View 1 Of eleve's Profiles -->
    <record id="view_eleve_eleve_search_1" model="ir.ui.view">
        <field name="nom">eleve.eleve.search.1</field>
        <field name="model">eleve.eleve</field>
        <field name="arch" type="xml">
            <search string="Informations Elève">
                <group col="10" colspan="4">
                    <field name="pid" />
                    <field name="eleve_nom" invisible="1" />
                    <field name="annee" />
                    <field name="sexe" />
                    <field name="ecole_id" placeholder="Ecole"/>
                </group>
                <newline />
                <group expand="0" string="Par groupe..." colspan="12" col="10">
                    <filter name="classe" string="Classe" help="Par Classe" context="{'groupe_par':'classe_id'}" />
                    <filter name="sexe" string="Sexe" help="Par Genre" context="{'groupe_par':'sexe'}" />
                    <filter name="nom" string="Nom" help="Par Nom" context="{'groupe_par':'eleve_nom'}" />
                    <filter name="ecole" string="Ecole" help="Par Ecole" context="{'groupe_par':'ecole_id'}" />
                    <filter name="etat" string="Etat" help="Par Status" context="{'groupe_par':'etat'}"/>
                </group>
                <separator/>
                <filter name="ancien" string="Ancien" domain="[('etat','=', 'ancien')]" help="Admission dans l'état confirmé"/>
                <filter name="mettre_fin" string="Terminé" domain="[('etat','=', 'mettre_fin')]" help="Admission en attente"/>
                <filter name="terminé" string="Terminé" domain="[('etat','=', 'terminé')]" help="Admission en état terminé"/>
            </search>
        </field>
    </record>
    <!-- Kanban View Of eleve's Profiles -->
    <record id="eleve_eleve_kanban_view" model="ir.ui.view">
        <field name="nom">Elèves KANBAN</field>
        <field name="model">eleve.eleve</field>
        <field name="arch" type="xml">
            <kanban default_group_by="classe_id" nocreate="1" >
                <field name="color"/>
                <field name="id"/>
                <field name="nom"/>
                <field name="email"/>
                <templates>
                    <t t-name="kanban-box">
                        <t t-set="color" t-value="kanban_color(record.color.raw_value || record.nom.raw_value)" />
                        <div t-att-class="color + (record.color.raw_value == 1 ? ' oe_kanban_color_alert' : '')">
                            <div class="oe_kanban_box oe_kanban_color_border">
                                <div class="oe_kanban_box_header oe_kanban_color_bgdark oe_kanban_color_border oe_kanban_draghandle">
                                    <table class="oe_kanban_table">
                                        <tr>
                                            <td class="oe_kanban_title1" align="left" valign="middle" style="padding-left:10px;">
                                                <h4>
                                                    <a type="open">
                                                        <field name="nom" />
                                                        <field name="nom_famille_eleve" />
                                                        <field name="eleve_nom" invisible="1" />
                                                        <field name="classe_id" invisible="1" />
                                                        <field name="ecole_id" invisible="1" />
                                                        <field name="sexe" invisible="1" />
                                                        <field name="etat" invisible="1" />
                                                    </a>
                                                </h4>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                                <div class="oe_kanban_box_content oe_kanban_color_bglight oe_kanban_box_show_onclick_trigger oe_kanban_color_border">
                                    <table class="oe_kanban_table">
                                        <tr width="300">
                                            <td valign="top" width="64" align="left">
                                                <a type="open">
                                                    <img heigh="80" width="80" t-att-src="kanban_image('eleve.eleve', 'photo', record.id.raw_value)" alt="image"/>
                                                </a>
                                            </td>
                                            <td valign="top" align="left" style="padding-left:10px;">
                                                <table>
                                                    <tr>
                                                        <td>ID Elève :</td>
                                                        <td style="padding-left:10px;">
                                                            <b>
                                                                <field name="pid" />
                                                            </b>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>Classe : </td>
                                                        <td style="padding-left:10px;">
                                                            <b>
                                                                <field name="classe_id" />
                                                            </b>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>Date D'Admission : </td>
                                                        <td style="padding-left:10px;">
                                                            <b>
                                                                <field name="date_admission" />
                                                            </b>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <center>
                                                    <b>
                                                        <field name="sexe" />
                                                    </b>
                                                </center>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                                <div class="oe_kanban_buttons_set oe_kanban_color_border oe_kanban_color_bglight oe_kanban_box_show_onclick">
                                    <div class="oe_kanban_left">
                                        <a role="button" string="Modifier" class="btn btn-sm btn-default fa fa-pencil" type="edit" groups="ecole.group_ecole_administration"/>
                                        <a role="button" string="Changer Couleur" icon="color-picker" type="color" name="color" />
                                    </div>
                                    <br class="oe_kanban_clear" />
                                </div>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>


    <!-- Form View OfRegistre D'admission Information -->
    <record id="view_eleve_eleve_form_2" model="ir.ui.view">
        <field name="nom">eleve.eleve.form.2</field>
        <field name="model">eleve.eleve</field>
        <field name="arch" type="xml">

            <form string="Registre d'admission">
                <header>
                    <button class="oe_highlight oe_stat_button" name="set_mettre_fin" string="Mettre fin" type="object" icon="fa-ban" etats="terminé" groups="ecole.group_ecole_administration"/>
                    <button class="oe_stat_button" name="set_ancien" string="Ancien" type="object" icon="fa-archive" etats="terminé" groups="ecole.group_ecole_administration" />
                    <button class="oe_stat_button" icon="fa-check" name="admission_terminé" type="object" string="Terminé" etats="Brouillon" groups="ecole.group_ecole_administration" />
                    <button class="oe_stat_button" icon="fa-ban" name="annuler_admission" type="object" string="Annuler" etats="Brouillon" groups="ecole.group_ecole_administration"/>
                    <field name="etat" widget="statusbar" statusbar_visible="Brouillon,annuler"/>
                </header>
                <sheet>
                    <div class="oe_inline">
                        <newline />
                        <separator string="Information Personalle" />
                        <field name="photo" widget="image" class="oe_avatar oe_left" style="max-width:100px;max-height:117px;" />
                        <div class="oe_title">
                            <group>
                                <field name="pid" nolabel="1" readonly="1"/>
                                <br />
                                <field name="nom" placeholder="Prénom" nolabel="1" required="1" attrs="{'readonly':[('etat','=','ancien')]}"/>
                                <br />
                                <field name="nom_famille_eleve" placeholder="Nom Famille" nolabel="1" attrs="{'readonly':[('etat','=','ancien')]}"/>
                                <br/>
                                <field name="active" invisible="1"/>
                            </group>
                        </div>
                    </div>
                    <group>
                        <group string="Ecole Information">
                            <field name="ecole_id" placeholder="Nom Ecole" required="1"
                                attrs="{'readonly':[('etat','=','ancien')]}" options="{&quot;no_open&quot;: True, &quot;no_create&quot;: True}"/>
                            <field name="annee" widget="selection" placeholder="Année Académique" readonly="1"/>
                            <field name="company_id" invisible="1"/>
                        </group>
                        <group string="Information Classe">
                            <field name="classe_id"  placeholder="Classe" domain="[('ecole_id','=',ecole_id)]" widget="selection" attrs="{'readonly':[('etat','=','ancien')]}"/>
                            <field name="division_id"  placeholder="Division" widget="selection" attrs="{'readonly':[('etat','=','ancien')]}" invisible="1"/>
                            
                        </group>
                    </group>
                    <group colspan="4">
                        <group>
                            <label for="street" string="Adresse" />
                            <div class="o_address_format">
                                <field name="street" placeholder="Rue..." class="o_address_street" attrs="{'readonly':[('etat','=','ancien')]}"/>
                                <field name="pays_id" placeholder="Pays" class="o_address_country,oe_no_button" required="1" options="{&quot;no_open&quot;: True}" attrs="{'readonly':[('etat','=','ancien')]}"/>
                                <field name="ville" placeholder="Ville" class="o_address_city" required="1" attrs="{'readonly':[('etat','=','ancien')]}"/>
                                <field name="etat_id" class="o_address_etat" domain="[('pays_id','=',pays_id)]" placeholder="Etat" options="{&quot;no_open&quot;: True}" required="1" attrs="{'readonly':[('etat','=','ancien')]}"/>

                            </div>
                        </group>
                        <group>
                            <field name="telephone" string="Numéro Télételephone" placeholder="Numéro Télételephone" attrs="{'readonly':[('etat','=','ancien')]}"/>
                            <field name="mobile" string="Numéro Portable" placeholder="Numéro Portable" attrs="{'readonly':[('etat','=','ancien')]}"/>
                            <field name="email" string="Adresse Email" placeholder="Adresse Email" String="Email" attrs="{'readonly':[('etat','=','ancien')]}"/>
                            <field name="website" string="Site Web" widget="url" placeholder="ex www.example.com" attrs="{'readonly':[('etat','=','ancien')]}"/>
                        </group>
                    </group>
                    <notebook colspan="4">
                        <page string="General">
                            <form string="General">
                                <sheet>
                                    <group string="Information Général">
                                        <group>
                                            <field name="sexe" placeholder="Sexe" attrs="{'readonly':[('etat','=','ancien')]}" required="1"/>
                                            <field name="date_de_naissance" placeholder="Date De Naissance" attrs="{'readonly':[('etat','=','ancien')]}"/>
                                            <field name="age" />
                                        </group>
                                        <group>
                                            <field name="langue_maternelle" widget="selection" placeholder="Langue Maternelle" attrs="{'readonly':[('etat','=','ancien')]}"/>
                                            <field name="date_admission" placeholder="Date D'Admission" attrs="{'readonly':[('etat','=','ancien')]}" readonly="1"/>
                                            <field name="etat_civil" placeholder="Sélectionnez le statut matrimonial" attrs="{'readonly':[('etat','=','ancien')]}"/>
                                        </group>
                                        <newline />
                                        <separator string="Détails des contacts d'urgence" colspan="4" />
                                        <newline />
                                        <group colspan="4" col="4">
                                            <field name="contact_telephone" attrs="{'readonly':[('etat','=','ancien')]}" placeholder="Numéro de Télételephone"/>
                                            <field name="contact_portable" attrs="{'readonly':[('etat','=','ancien')]}" placeholder="Numéro de Portable"/>
                                        </group>
                                    </group>
                                </sheet>
                            </form>
                        </page>
                        <page string="Parents">
                            <field name="parent_id" attrs="{'readonly':[('etat','=','ancien')]}"
                                context="{'form_view_ref':'ecole.view_parent_form','default_parent_ecole':1}"
                                options="{&quot;no_open&quot;: True, &quot;no_create&quot;: True}"/>
                        </page>

                        <page string="Ecole précédente">
                            <field name="precedent_ecole_ids" nolabel="1" colspan="4" attrs="{'readonly':[('etat','=','ancien')]}">
                                <tree string="Details Ecole précédente">
                                    <field name="nom" />
                                    <field name="numero_enseignant" />
                                    <field name="date_admission" />
                                    <field name="date_sortie" />
                                    <field name="course_id" widget="selection" />
                                </tree>
                                <form string="Details Ecole précédente">
                                    <separator string="Details Ecole précédente" />
                                    <group col="4" colspan="4">
                                        <field name="nom" placeholder="Nom Ecole" />
                                        <field name="numero_enseignant" placeholder="N ° d'enregistrement" />
                                        <field name="date_admission" placeholder="Date D'Admission "/>
                                        <field name="date_sortie" placeholder="Date de sortie"/>
                                        <field name="course_id" placeholder="Cours" widget="selection"/>
                                        <newline />
                                        <separator string="Details Matière" />
                                        <field name="ajouter_mat" nolabel="1" colspan="4"/>
                                    </group>
                                </form>
                            </field>
                        </page>
                        <page string="Détails de famille">
                            <field name="famille_con_ids" nolabel="1" colspan="4" attrs="{'readonly':[('etat','=','ancien')]}">
                                <tree string="Coordonnées de la famille">
                                    <field name="nom_relative"/>
                                    <field name="relation" />
                                    <field name="telephone" />
                                </tree>
                                <form string="Coordonnées de la famille">
                                    <group>
                                        <group>
                                            <field name="nom_rel" placeholder="Nom" />
                                            <field name="nom_ele" placeholder="Créez ou Selectionnez Nom" attrs="{'invisible':[('nom_rel','!=','exist')]}"
                                                        options="{&quot;no_create&quot;: True}"/>
                                            <field name="nom" placeholder="Nom" attrs="{'invisible':[('nom_rel','!=','new')]}"/>
                                            <field name="relation" placeholder="Entrez ou sélectionnez Relation" />
                                            <field name="nom_relative" invisible="1"/>
                                        </group>
                                        <group>
                                            <field name="telephone" placeholder="Numéro De Télételephone" />
                                            <field name="email" placeholder="Adresse Email " />
                                        </group>
                                    </group>
                                </form>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
