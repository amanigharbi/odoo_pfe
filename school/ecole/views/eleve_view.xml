                    <filter name="sexe" string="Sexe" domain="[]" help="par sexe" context="{'groupe_par':'sexe'}" />
                    <filter name="nom" string="Nom" domain="[]" help="par Class" context="{'groupe_par':'eleve_nom'}" />
