<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">

        <!-- Sequences For eleve.code (eleve Code) -->

        <record id="seq_eleve_code" model="ir.sequence">
            <field name="name">eleve code</field>
            <field name="code">eleve.code</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences For eleve.registration (Registration Code) -->

        <record id="seq_eleve_reg" model="ir.sequence">
            <field name="name">eleve registration code</field>
            <field name="code">eleve.registration</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences For eleve.eleve (Personal Identification Number) -->

        <record id="seq_eleve_eleve" model="ir.sequence">
            <field name="name">eleve</field>
            <field name="code">eleve.eleve</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>

        </record>

        <!-- Sequences For eleve.document (eleve Documents) -->

        <record id="seq_eleve_document" model="ir.sequence">
            <field name="name">Document</field>
            <field name="code">eleve.document</field>
            <field name="prefix">F</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences For document.type (Document Type) -->

        <record id="seq_eleve_sequence" model="ir.sequence">
            <field name="name">sequence</field>
            <field name="code">document.type</field>
            <field name="padding">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
